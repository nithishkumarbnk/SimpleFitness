@model IEnumerable<WebApplication1.Models.User>

@{
    ViewData["Title"] = "Users";
}

<h1>Users</h1>


<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Role</th>
            <th>Email</th>
            <th>Age</th>
            <th>Gender</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Name</td>
                <td>@user.Role</td>
                <td>@user.Email</td>
                <td>@user.Age</td>
                <td>@user.Gender</td>
                <td>
                   
                </td>
            </tr>
        }
    </tbody>
</table>
